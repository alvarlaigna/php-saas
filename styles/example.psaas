@var forecolor = #ff0000;
@var backcolor = #00ff00;

@mixin alert
	border: 1px solid @backcolor
	height: 100%

@mixin clear
	content: ''

body 
	color: @forecolor
	background: @backcolor
	
	div 
		margin: 0
		
		:hover
			border: 1px solid red
		
#test
	=alert
	=clear
	color: @forecolor
	
@import 'styles/example2'